{
  "name": "i18n-on-remix",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky & panda codegen",
    "build": "remix build",
    "dev": "remix dev --manual -c \"bun run start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./public  --ip ::",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "format": "prettier --write \"./app/**/*.{ts,tsx}\"",
    "fix": "bun run format && bun run lint",
    "test": "vitest"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.5.1",
    "@remix-run/cloudflare-pages": "^2.5.1",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "hono": "^4.2.5",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "i18next-resources-to-backend": "^1.2.1",
    "isbot": "^4.1.0",
    "nanoid": "^5.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-i18next": "^6.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@pandacss/dev": "^0.29.1",
    "@pandacss/eslint-plugin": "^0.1.1",
    "@remix-run/dev": "^2.5.1",
    "@testing-library/react": "^16.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.12.11",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^14.12.0",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.1.0",
    "vitest": "^1.6.0",
    "wait-on": "^7.2.0",
    "wrangler": "3.50.0"
  }
}
