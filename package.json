{
  "name": "i18n-on-remix",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky & panda codegen",
    "build": "remix build",
    "dev": "remix dev --manual -c \"bun run start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./public --compatibility-date=2024-06-30  --ip ::",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "format": "prettier --write \"./app/**/*.{ts,tsx}\"",
    "fix": "bun run format && bun run lint",
    "check-lock": "diff <(bun bun.lockb) yarn.lock"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.10.0",
    "@remix-run/cloudflare-pages": "^2.10.0",
    "@remix-run/css-bundle": "^2.10.0",
    "@remix-run/react": "^2.10.0",
    "hono": "^4.4.10",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "i18next-resources-to-backend": "^1.2.1",
    "isbot": "^4.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-i18next": "^6.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "@pandacss/dev": "^0.29.1",
    "@pandacss/eslint-plugin": "^0.1.9",
    "@remix-run/dev": "^2.10.0",
    "@testing-library/react": "^16.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "shell-quote": "^1.8.1",
    "wait-on": "^7.2.0",
    "wrangler": "^3.62.0"
  }
}
